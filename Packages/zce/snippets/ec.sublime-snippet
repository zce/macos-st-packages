const router = module.exports = Router()
// pathname starts with
router.prefix = '/${1:${TM_FILENAME/(.+)\..+|.*/$1/:demo}}'

/**
 * GET /
 */
router.get('/', (req, res) => {
  res.render('${2:index}')
})
